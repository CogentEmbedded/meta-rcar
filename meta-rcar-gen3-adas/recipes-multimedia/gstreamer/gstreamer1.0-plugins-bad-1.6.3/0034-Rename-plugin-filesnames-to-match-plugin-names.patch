From 62d559fa839ba621df22d5713a5f4e1765d7beb9 Mon Sep 17 00:00:00 2001
From: Nicolas Dufresne <nicolas.dufresne@collabora.com>
Date: Sat, 4 Mar 2017 11:03:53 -0500
Subject: [PATCH 34/80] Rename plugin filesnames to match plugin names

- libgstkmssink.so -> libgstkms.so

https://bugzilla.gnome.org/show_bug.cgi?id=779344
---
 sys/kms/Makefile.am | 12 ++++++------
 sys/kms/meson.build |  2 +-
 2 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/sys/kms/Makefile.am b/sys/kms/Makefile.am
index daf2ff710..a97cad1e4 100644
--- a/sys/kms/Makefile.am
+++ b/sys/kms/Makefile.am
@@ -1,13 +1,13 @@
-plugin_LTLIBRARIES = libgstkmssink.la
+plugin_LTLIBRARIES = libgstkms.la
 
-libgstkmssink_la_SOURCES = 			\
+libgstkms_la_SOURCES = 			\
 	gstkmssink.c				\
 	gstkmsutils.c				\
 	gstkmsallocator.c			\
 	gstkmsbufferpool.c			\
 	$(NUL)
 
-libgstkmssink_la_CFLAGS = 			\
+libgstkms_la_CFLAGS = 			\
 	$(GST_PLUGINS_BASE_CFLAGS) 		\
 	$(GST_BASE_CFLAGS) 			\
 	$(GST_VIDEO_CFLAGS)			\
@@ -16,7 +16,7 @@ libgstkmssink_la_CFLAGS = 			\
 	$(KMS_DRM_CFLAGS) 			\
 	$(NULL)
 
-libgstkmssink_la_LIBADD = 			\
+libgstkms_la_LIBADD = 			\
 	$(GST_PLUGINS_BASE_LIBS) 		\
 	$(GST_BASE_LIBS) 			\
 	$(GST_VIDEO_LIBS)			\
@@ -25,11 +25,11 @@ libgstkmssink_la_LIBADD = 			\
 	$(KMS_DRM_LIBS) 			\
 	$(NULL)
 
-libgstkmssink_la_LDFLAGS = 			\
+libgstkms_la_LDFLAGS = 			\
 	$(GST_PLUGIN_LDFLAGS)			\
 	$(NULL)
 
-libgstkmssink_la_LIBTOOLFLAGS = 		\
+libgstkms_la_LIBTOOLFLAGS = 		\
 	$(GST_PLUGIN_LIBTOOLFLAGS) 		\
 	$(NULL)
 
diff --git a/sys/kms/meson.build b/sys/kms/meson.build
index 05d49c2ab..e8865f05f 100644
--- a/sys/kms/meson.build
+++ b/sys/kms/meson.build
@@ -8,7 +8,7 @@ kmssink_sources = [
 libdrm_dep = dependency('libdrm', version : '>= 2.4.55', required : false)
 
 if libdrm_dep.found()
-  gstkmssink = library('gstkmssink',
+  gstkmssink = library('gstkms',
     kmssink_sources,
     c_args : gst_plugins_bad_args,
     include_directories : [configinc],
-- 
2.11.0

