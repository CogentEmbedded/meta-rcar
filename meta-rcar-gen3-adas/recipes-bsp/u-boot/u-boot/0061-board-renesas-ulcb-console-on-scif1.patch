From 3418356836db17fc85784b38071f6f91e41b8c45 Mon Sep 17 00:00:00 2001
From: Valentine Barshak <valentine.barshak@cogentembedded.com>
Date: Fri, 30 Nov 2018 01:17:31 +0300
Subject: [PATCH 09/11] arm: dts: ulcb: Set H3ULCB-HAD console on SCIF1

This sets ULCB console on SCIF1 for H3ULCB board.

Signed-off-by: Valentine Barshak <valentine.barshak@cogentembedded.com>
---
 arch/arm/dts/ulcb.dtsi | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm/dts/ulcb.dtsi b/arch/arm/dts/ulcb.dtsi
index 24f3b42..17a0578 100644
--- a/arch/arm/dts/ulcb.dtsi
+++ b/arch/arm/dts/ulcb.dtsi
@@ -16,7 +16,7 @@
 	model = "Renesas R-Car Gen3 ULCB board";
 
 	aliases {
-		serial0 = &scif2;
+		serial0 = &scif1;
 		ethernet0 = &avb;
 	};
 
@@ -285,9 +285,9 @@
 		function = "i2c2";
 	};
 
-	scif2_pins: scif2 {
-		groups = "scif2_data_a";
-		function = "scif2";
+	scif1_pins: scif1 {
+		groups = "scif1_data_a";
+		function = "scif1";
 	};
 
 	scif_clk_pins: scif_clk {
@@ -380,8 +380,8 @@
 	status = "okay";
 };
 
-&scif2 {
-	pinctrl-0 = <&scif2_pins>;
+&scif1 {
+	pinctrl-0 = <&scif1_pins>;
 	pinctrl-names = "default";
 
 	status = "okay";
-- 
2.7.4

