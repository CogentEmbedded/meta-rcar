+	DEF_MOD("imr2",			 821,	R8A77980_CLK_S2D1),
+	DEF_MOD("imr1",			 822,	R8A77980_CLK_S2D1),
+	DEF_MOD("imr0",			 823,	R8A77980_CLK_S2D1),
 	DEF_MOD("imp3",			 824,	R8A77980_CLK_S1D1),
