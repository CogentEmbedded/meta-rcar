+++ b/arch/arm64/boot/dts/renesas/r8a7798-v3hsk.dts
@@ -398,7 +398,7 @@
 				read-only;
 			};
 			bl31@001C0000 {
-				reg = <0x001C0000 0x460000>;
+				reg = <0x001C0000 0x480000>;
 				read-only;
 			};
 			uboot@00640000 {
